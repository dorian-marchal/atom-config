{
  "name": "stylelint",
  "version": "6.1.0",
  "description": "Modern CSS linter",
  "keywords": [
    "css",
    "csslint",
    "lint",
    "linter",
    "stylelint"
  ],
  "authors": [
    "David Clark",
    "Maxime Thirouin",
